{
  "name": "@apps/backend",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "start:dev": "NODE_ENV=development node --inspect --no-warnings=ExperimentalWarning --experimental-specifier-resolution=node --loader ts-node/esm/transpile-only src/index.ts",
    "build:clean": "rimraf dist",
    "build": "tsc --build tsconfig.prod.json",
    "build:dev": "tsc --build tsconfig.dev.json",
    "test:unit": "vitest .unit.test.ts --config vitest.unit.config.js",
    "test:unit:run": "npm run test:unit -- --run",
    "test:integration": "NODE_ENV=test vitest --config vitest.integration.config.js .integration.test.ts",
    "test:integration:run": "npm run test:integration -- --run",
    "test:e2e": "NODE_ENV=test vitest --config vitest.e2e.config.js",
    "test:e2e:run": "npm run test:e2e -- --run",
    "bench": "vitest bench",
    "lint": "eslint . --ext .ts --cache --max-warnings 0",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.441.0",
    "@common/contracts": "*",
    "@fastify/cors": "^8.4.2",
    "@fastify/helmet": "^11.1.1",
    "@fastify/swagger": "^8.12.1",
    "@fastify/swagger-ui": "^1.10.2",
    "@fastify/type-provider-typebox": "^3.5.0",
    "@sinclair/typebox": "^0.31.26",
    "archiver": "^6.0.1",
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^9.2.2",
    "config": "^3.3.9",
    "fastify": "^4.24.3",
    "fluent-ffmpeg": "^2.1.2",
    "http-status-codes": "^2.3.0",
    "inversify": "^6.0.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.0.1",
    "node-fetch": "3.3.2",
    "pino": "^8.16.2",
    "sharp": "^0.33.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@common/tests": "*",
    "@types/archiver": "^6.0.2",
    "@types/bcrypt": "^5.0.2",
    "@types/config": "^3.3.3",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.3",
    "@types/uuid": "^9.0.7"
  },
  "volta": {
    "node": "20.10.0"
  }
}
